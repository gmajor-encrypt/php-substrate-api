{
  "name": "gmajor/php-substrate-api",
  "description": "PHP Substrate RPC Api",
  "keywords": [
    "substrate",
    "http",
    "rpc"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-gmp": "*",
    "ext-json": "*",
    "ext-sodium": "*",
    "gmajor/sr25519-bindings": "v1.*",
    "gmajor/substrate-codec-php": "v1.*",
    "textalk/websocket": "dev-master",
    "tuupola/base58": "2.x-dev",
    "netresearch/jsonmapper": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^9.3.0"
  },
  "autoload": {
    "psr-4": {
      "Rpc\\": "src/Rpc/"
    }
  }
}
